{
  "schemaVersion": 1,
  "id": "mythiccontrols",
  "version": "${version}",
  "name": "mythiccontrols",
  "description": "Allows the Minecraft client to send MythicControl keypresses to the server.",
  "authors": [
    "alvindimas05"
  ],
  "contact": {
    "homepage": "https://github.com/ASangarin/MythicControls/wiki",
    "issues": "https://github.com/alvindimas05/MythicControls/issues",
    "sources": "https://github.com/alvindimas05/MythicControls"
  },
  "license": "MIT",
  "icon": "assets/mythiccontrols/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "org.alvindimas05.mythiccontrols.fabric.MythicControlsFabric"
    ]
  },
  "mixins": [
    "mythiccontrols.mixins.json",
    "mythiccontrols-fabric.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=1.20"
  }
}
